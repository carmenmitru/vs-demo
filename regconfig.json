{
  "core": {
    "workingDir": ".reg",
    "actualDir": "./visual-reg/actual/",
    "thresholdRate": 0,
    "addIgnore": true,
    "ximgdiff": {
      "invocationType": "client"
    }
  },
  "plugins": {
    "reg-keygen-git-hash-plugin": true,
    "reg-notify-slack-plugin": {
      "webhookUrl": "${SLACK_WEBHOOK}"
    },
    "reg-publish-s3-plugin": {
      "bucketName": "${BUCKET_NAME}"
    }
  }
}
